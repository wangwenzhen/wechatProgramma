/*list*/
.cells {
  margin-top: 12px;
  background-color: #FFFFFF;
  overflow: hidden;
  position: relative;
}
.cells:before {
  content: " ";
  position: absolute;
  left: 0;
  top: 0;
  right: 0;
  height: 1px;
  border-top: 1px solid #d8d8d8;
  color: #d8d8d8;
  transform-origin: 0 0;
  transform: scaleY(0.5);
}
.cells:after {
  content: " ";
  position: absolute;
  left: 0;
  bottom: 0;
  right: 0;
  height: 1px;
  border-bottom: 1px solid #d8d8d8;
  color: #d8d8d8;
  transform-origin: 0 100%;
  transform: scaleY(0.5);
}
.cell {
  padding: 12px;
  position: relative;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
  align-items: center;
}
.cell:before {
  content: " ";
  position: absolute;
  top: 0;
  right: 0;
  height: 1px;
  border-top: 1px solid #e6e6e6;
  color: #e6e6e6;
  transform-origin: 0 0;
  transform: scaleY(0.5);
  left: 12px;
}
.cell:first-child:before {
  display: none;
}
.cell.act {
  -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
  color: inherit;
}
.cell.act:active {
  background-color: #ECECEC;
}
.cell-bd {
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
}
.cell-ft {
  padding-right: 12px;
  position: relative;
}
.cell-ft:after {
  content: " ";
  display: inline-block;
  height: 8px;
  width: 8px;
  border-width: 2px 2px 0 0;
  border-color: #d8d8d8;
  border-style: solid;
  transform: matrix(0.71, 0.71, -0.71, 0.71, 0, 0);
  top: -2px;
  position: absolute;
  top: 50%;
  margin-top: -4px;
  right: 2px;
} 
.cell.more {
  color: #969696;
  text-align: center;
  padding: 6px;
}
.cell.more:before {
  left: 0;
}
.cells.line .cell:before {
  left: 0;
}
.cell-ico > image {
  width: 18px;
  display: block;
  margin-right: 5px;
}
.cell-vcode {
  padding: 0px 0px 0px 0px !important;
}
.cell-verify {
  border-left: 1px solid #d8d8d8;
  padding: 18px 12px;
  text-align: center;
  color: #446b81;
}

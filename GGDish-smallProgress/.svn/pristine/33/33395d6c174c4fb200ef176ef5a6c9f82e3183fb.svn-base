page{
    background-color: #f6f6f6;
}
.place-order-top-view{
    text-align: center;
    padding: 15px 0;
    position: relative;
    background-color: #FFFFFF;
    border-bottom: 1px solid #d8d8d8;
}
.place-order-top-view image{
    width: 203px;
    display: inline-block;

}
.place-order-tip{
    padding-top: 13px;
    font-size: 15px;
    font-weight:400;
}
.place-orderId-view{
    position: absolute;
    color: #FFFFFF;
    top: 34px;
    width: 100%;
}
.place-orderId{
    font-weight: 500;
    font-size: 30px;
    line-height: 34px;
    position: relative;
    top: 9px;
}
.return-ticket{
    margin-top: 13px;
    padding: 15px 32px;
    border-top: 1px solid #d8d8d8;
    background-color: #FFFFFF;
    border-bottom: 1px solid #d8d8d8;
}
.returnTicketTip{
    color: #969696;
    font-size: 15px;
    margin-bottom: 10px;

}
.return-ticket-bg{
    position: relative;
    margin-bottom: 10px;
}
.return-ticket-bg image{
    width: 100%;
    height: 90px;
    display: inline-block;
}
.return-ticket-info{
    position: absolute;
    align-items:center;
    display: flex;
    top: 0;
    width: 100%;
    height: 100%;

}
.return-ticket-left{
    width: 45%;
    font-size: 60rpx;
    text-align: center;
    top: -4px;
}
/*代金券*/
.return-ticket-vouchers::before{
    content: "￥";
    font-size: 30rpx;
    position: relative;
    top: 2px;
}
/*折扣券*/
.return-ticket-discount::after{
    content: "折";
    font-size: 30rpx;
    position: relative;
    top: 2px;
}
.return-ticket-right{
    flex: 1;
    margin-left: 8px;

}
.return-kind{
    font-size: 36rpx;
    margin-bottom: 4px;
}
.return-ticket-num{
    position: absolute;
    font-size: 17px;
    text-align: center;
    line-height: 25px;
    font-weight: 400;
    width: 25px;
    height: 25px;
    top: 50%;
    right: -12px;
    border-radius: 25px;
    transform: translateY(-50%);
    color: #FFFFFF;
    background-color: #ffcc00;
}
.return-ticket-account{
    padding-top: 6px;
    text-align: center;
    font-weight: 500;
    font-size: 13px;
}
.palce-order-info{
    margin-top: 13px;
    border-top: 1px solid #d8d8d8;
    border-bottom: 1px solid #d8d8d8;
    padding: 9px 14px;
    background-color: #FFFFFF;
    color: #969696;
}
.palce-order-info > view{
    padding: 6px 0;
}
.palce-order-info .right{
    flex: 1;
}
.info-item{
    display: flex;
}
.priceUnit::before{
    content: "￥";
    position: relative;
    top: 1px;
}
.place-order-bot-view{
    padding: 27px 17px;
}
.input-receive{
    line-height:29px;
    padding: 5px 12px;
    border: 1px solid #d8d8d8;
    width: 60%;
    
}
.receive-default{
    color: #d8d8d8;
    vertical-align: middle;
    
}
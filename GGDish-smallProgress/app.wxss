/*toast 样式*/
.tip {
  position: absolute;
  top: 0;
  height: 100%;
  width: 100%;
  z-index: 10000;
  
}

/*.tip image {
  width: 20px;
  height: 20px;
  display: inline-block;
}*/

.tip > view {
  margin: 0 auto;
  max-width: 120px;
  background: rgba(40, 40, 40, 0.75);
  border-radius: 0.3rem;
  color: white;
  text-align: center;
  padding: 8px 0;
  top: 40%;
  position: relative;
  opacity: 0;
}

.fadeUp {
  animation: fadeUp; /*动画名称*/
  animation-duration: 2s; /*动画持续时间*/

}

@keyframes fadeUp {
  0% {
    opacity: 0; /*初始状态 透明度为0*/
    transform: translateY(30px);
  }

  35% {
    opacity: 1; /*中间状态 透明度为0*/
    transform: translateY(0);
  }

  65% {
    opacity: 1; /*中间状态 透明度为0*/
    transform: translateY(0);
  }

  100% {
    opacity: 0; /*结尾状态 透明度为1*/
    transform: translateY(30px);
  }
}

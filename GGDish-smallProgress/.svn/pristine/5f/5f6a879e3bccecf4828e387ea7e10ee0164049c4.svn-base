/* pages/order/chooseCoupon/chooseCoupon.wxss */
.cChoose{

}
.coupon-lay{
     position: absolute;
     width: 100%;
     top: 0;
     bottom: 110rpx;
}
.sure-lay{
    position: absolute;
    height: 110rpx;
    bottom: 0;
    width: 100%;
    background: #fff;

}
.sure-lay.cells{
    margin: 0;
}
.sure-lay.cells::after{
    display: none;
}
.sure-lay .btn{
    padding: 20rpx 0;
    margin: 16rpx 24rpx 12rpx 24rpx;
}
.item-coupons{
    width: 85%;
    margin: 0 auto;
    margin-top:38rpx;  
    display:flex;
    position:relative;
}
.couponInfo{
 font-size:48rpx;
}
.cash{
     color: #FFAC0D;
}
.discount{
     color: #34AB2A;
}
.discount .couponInfo::after{
    content: "折";
    font-size: 25rpx;
}

.cash .couponInfo::before{
    content: "¥";
    font-size: 25rpx;
}
.couponType{
     font-size:32rpx;
    margin-top: 35rpx;
}
.item-pay{
    display:flex;
}
.coupons-left{
    line-height:181rpx;
    width:45%;
    text-align: center;
    z-index: 1;
}
.coupons-right{
    flex:1;
    height:181rpx;
    padding-left:30rpx;
    z-index: 1;
}
.coupon-tip{
    color:#969696;
    font-size:24rpx;
}
.coupon-title{
    margin-left: 20rpx;
    margin-top: 20rpx;
    color: #969696;
    font-size: 20rpx;
}
.coupon-img{
    width:100%;
    height:182rpx;
    position:absolute;
}
.selector{
    width: 50rpx;
    height: 50rpx;
    position: absolute;
    left: -20rpx;
    top: -20rpx;
    z-index: 5;
}
.notUse .couponType,
.notUse .couponInfo{ 
    color: #969696;
}
.sure-order-container{
    position: absolute;
    bottom:0;
    left: 0;
    right: 0;
    z-index: 1;
}
.sure-order-container > .cell{
    padding: 0 0  0 24rpx;
    height: 100rpx;
}
.cmtOrder .sure-order-container.cells{
    overflow:visible;
}
.sure-order{
    padding-top: 0;
    padding-bottom: 0;
    padding-left: 33rpx;
    padding-right: 33rpx;
    line-height: 100rpx;
}
.should-pay{
    flex: 1;
    font-size: 32rpx;
    color: #CF112D;
    text-align:right;
    margin-right: 37rpx;
}
.discount{
    
    font-size: 26rpx;
    color: #444444;
    visibility:visible;
}
.return-coupons{
    line-height:20rpx;
    position:absolute;
    width: 100%;
    padding-bottom: 18rpx;
    top: -45rpx;
    text-align: center;
    z-index: 2;
}
.return-coupons-content{
    display:inline-block;
}
.return-coupons-content::before{
    content: '返券';
    border: #CF112D 1px solid;
    font-size: 20rpx;
    line-height:20rpx;
    color: #CF112D;
    border-radius: 8rpx;
    padding: 0 2rpx;
}
.integral{
    display:inline-block;
}
.integral::before{
    content: '积分';
    border: #CF112D 1px solid;
    color: #CF112D;
    line-height:20rpx;
    font-size: 20rpx;
    border-radius: 8rpx;
    padding: 0 2rpx;
}

